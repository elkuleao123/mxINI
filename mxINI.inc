				inv_info[playerid][in_afrotZ][i]=fRotZ;
				inv_info[playerid][in_afscaleX][i]=fScaleX;
				inv_info[playerid][in_afscaleY][i]=fScaleY;
				inv_info[playerid][in_afscaleZ][i]=fScaleZ;
